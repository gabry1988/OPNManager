{
  "identifier": "com.opnmanager.mobile",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "distDir": "../dist",
    "devPath": "http://localhost:5173"
  },
  "app": {
    "windows": [
      {
        "title": "OPNManager",
        "width": 800,
        "height": 600
      }
    ]
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/icon.png",
      "icons/icon.ico"
    ],
    "android": {
      "minSdkVersion": 24
    }
  },
  "security": {
    "csp": null
  }
}

{
  "identifier": "com.opnmanager.mobile",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173"
  },
  "app": {
    "windows": [
      {
        "title": "OPNManager",
        "width": 800,
        "height": 600
      }
    ]
  },
  "bundle": {
    "active": true,
    "icon": ["icons/icon.png", "icons/icon.ico"],
    "android": {
      "package": "com.opnmanager.mobile",
      "minSdkVersion": 24,
      "targetSdkVersion": 34
    }
  },
  "allowlist": {
    "all": true
  }
}